  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "dev": "ts-node-dev --transpile-only --ignore-watch node_modules --respawn index.ts",
    "transpile": "npx tsc",
    "rimraf": "rm -rf dist",
    "start": "npm run rimraf && npm run build && node dist/index.js",
    "migrate": "npx prisma migrate deploy && npx prisma generate",
    "migrate:2": "npx prisma generate",
    "prod": "npx tsc && npm run copy:libs && node -r newrelic dist/index.js",
    "jack": "npm run build && node dist/src/utils/rm_user.js",
    "copy:libs": "cpx \"src/libs/**/*\" dist/src/libs"
  },
